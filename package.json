{
  "name": "tiny-glob",
  "version": "0.2.2",
  "description": "Tiny and extremely fast globbing",
  "main": "./index.js",
  "files": [
    "*.js",
    "./lib/*.js",
    "./src/*.js",
    "./index.d.ts"
  ],
  "scripts": {
    "bench": "node bench",
    "test": "tape test/*.js | tap-spec",
    "prepublish": "babel src -d lib"
  },
  "author": {
    "name": "Terkel Gjervig",
    "email": "terkel@terkel.com",
    "url": "https://terkel.com"
  },
  "keywords": [
    "glob",
    "globbing",
    "patterns",
    "wildcard",
    "pattern-matching",
    "expansion"
  ],
  "repository": "https://github.com/terkelg/tiny-glob",
  "license": "MIT",
  "dependencies": {
    "globalyzer": "^0.1.0",
    "globrex": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "types": "./index.d.ts",
  "engines": {
    "node": ">= 6.0.0"
  }
}
